{
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgres;Database=${POSTGRES_DB};Username=${POSTGRES_USER};Password=${POSTGRES_PASSWORD}",
    "Redis": "host.docker.internal:6370,password=${REDIS_PASSWORD}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "SeedTestUsers": false,
  "Email": {
    "Host": "${MAILING_HOST}",
    "Port": "${MAILING_PORT}",
    "EnableSsl": "${MAILING_ENABLE_SSL}",
    "UserName": "${MAILING_USERNAME}",
    "Password": "${MAILING_PASSWORD}",
    "From": "${MAILING_FROM}"
  }
}
